<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>
<shortcuts>
  
  <skipIfNotSupported/>

  <programGroup defaultName="Spring by Pivotal"
                location="applications"/>

  <shortcut
    name="Spring Tool Suite %sts.version%"
    target="$INSTALL_PATH\sts-%sts.version%\STS.exe"
    commandLine=""
    workingDirectory="$INSTALL_PATH\sts-%sts.version%"
    description="Spring Tool Suite"
    initialState="normal"
    programGroup="yes"
    desktop="no"
    applications="no"
    startMenu="no"
    startup="no">
    <createForPack name="Spring Tool Suite %sts.version%" />
  </shortcut>

</shortcuts>

