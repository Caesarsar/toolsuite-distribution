<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <errTitle>Installer - Groovy-Grails Tool Suite</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl>http://forum.springsource.org/forumdisplay.php?f=32</supportUrl>
  <customProcName>true</customProcName>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>Eclipse.ico</icon>
  <jre>
    <minVersion>1.5.0</minVersion>
    <jdkPreference>preferJre</jdkPreference>
  </jre>
  <versionInfo>
    <fileVersion>%sts.major.version%.0</fileVersion>
    <txtFileVersion>%sts.version%</txtFileVersion>
    <fileDescription>Installer - Groovy-Grails Tool Suite</fileDescription>
    <copyright>Copyright (c) 2007, 2012 SpringSource.  All rights reserved.</copyright>
    <productVersion>%sts.major.version%.0</productVersion>
    <txtProductVersion>%sts.version%</txtProductVersion>
    <productName>Groovy-Grails Tool Suite</productName>
    <companyName>SpringSource</companyName>
    <internalName>%sts.internal.name%</internalName>
    <originalFilename>%sts.file.name%</originalFilename> 
  </versionInfo>
</launch4jConfig>
